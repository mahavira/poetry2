    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "postcss-loader": "^0.6.0",
    "react-transform-hmr": "^1.0.0",
    "rimraf": "^2.4.3",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.8",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.2.0",
